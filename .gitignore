# GSCMS (GDE SciBOTICS Competition Management System)
# .gitignore

# ==============================================
# PHP & Composer Dependencies
# ==============================================
/vendor/
composer.phar

# ==============================================
# Configuration Files (contains sensitive data)
# ==============================================
.env
.env.local
.env.*.local
config/database.php
config/app.php
config/mail.php
config/services.php

# ==============================================
# Logs & Cache
# ==============================================
/logs/
/storage/logs/*.log
/storage/cache/
/storage/framework/cache/
/storage/framework/sessions/
*.log
error_log
access.log

# ==============================================
# Uploaded Files & User Content
# ==============================================
/public/uploads/*
!/public/uploads/.gitkeep
/public/uploads/consent_forms/*
!/public/uploads/consent_forms/.gitkeep
/public/uploads/team_submissions/*
!/public/uploads/team_submissions/.gitkeep
/storage/uploads/
*.pdf
*.doc
*.docx
*.xls
*.xlsx

# ==============================================
# Temporary Files
# ==============================================
/tmp/
/temp/
*.tmp
*.temp
*.swp
*.swo
*~

# ==============================================
# IDE & Editor Files
# ==============================================
# PhpStorm
.idea/
*.iml
*.iws

# Visual Studio Code
.vscode/
*.code-workspace

# Sublime Text
*.sublime-project
*.sublime-workspace

# ==============================================
# Operating System Files
# ==============================================
# macOS
.DS_Store
.AppleDouble
.LSOverride
Icon?
._*
.Spotlight-V100
.Trashes

# Windows
Thumbs.db
Desktop.ini
$RECYCLE.BIN/

# Linux
.fuse_hidden*
.directory
.Trash-*
.nfs*

# ==============================================
# Development & Testing
# ==============================================
# PHPUnit
.phpunit.result.cache
/phpunit.xml
/coverage/
/build/

# Node.js (if using for frontend build tools)
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
package-lock.json
yarn.lock

# ==============================================
# Backup Files
# ==============================================
*.backup
*.bak
*.old
*.orig
backup/
backups/

# ==============================================
# Database Files
# ==============================================
*.sqlite
*.sqlite3
*.db

# SQL dumps (but allow schema and migration files)
*.sql
!database/migrations/*.sql
!database/seeds/**/*.sql

# ==============================================
# Security & Keys
# ==============================================
*.key
*.pem
*.p12
*.pfx
*.crt
*.cer
*.csr
id_rsa
id_dsa
*.pub

# ==============================================
# Archives & Compressed Files
# ==============================================
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip

# ==============================================
# Deployment Files (Critical - Keep Out of Git)
# ==============================================
# Local deployment preparation directory
# Contains production configs and sensitive data
local_deployment_prep/

# Hosting-specific files
.htaccess.local
.htaccess.staging
.htaccess.production
deploy.php
deployment/
.deploy/

# ==============================================
# Application Specific
# ==============================================
# Competition specific temporary files
/storage/competition_data/
/storage/exports/
/public/competition_photos/*
!/public/competition_photos/.gitkeep

# Generated documentation
/docs/generated/
/api-docs/

# Compiled assets (if using build tools)
/public/css/compiled/
/public/js/compiled/
/public/mix-manifest.json